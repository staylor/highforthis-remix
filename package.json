{
  "name": "highforthis",
  "version": "0.0.0",
  "description": "High for This",
  "repository": {
    "type": "git",
    "url": "https://github.com/staylor/highforthis"
  },
  "author": "Scott Taylor <scott.c.taylor@mac.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "dependencies": {
    "@apollo/client": "3.7.3",
    "@remix-run/express": "1.9.0",
    "@remix-run/node": "1.9.0",
    "@remix-run/react": "1.9.0",
    "@remix-run/serve": "1.9.0",
    "@remix-run/server-runtime": "1.9.0",
    "classnames": "2.3.2",
    "compression": "1.7.4",
    "draft-js": "0.11.7",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "filesize": "10.0.6",
    "forever": "4.0.3",
    "graphql": "16.6.0",
    "http-proxy-middleware": "2.0.6",
    "lodash.debounce": "4.0.8",
    "qs": "6.11.0",
    "query-types": "0.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redraft": "0.10.2",
    "remix-auth": "3.4.0",
    "remix-auth-form": "1.3.0",
    "rimraf": "3.0.2",
    "tiny-invariant": "1.3.1"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "yarn clean",
    "build:css": "run-s \"generate:css:* --minify\"",
    "build:remix": "env-cmd -e prod remix build",
    "clean": "rimraf build public/build",
    "dev": "yarn clean && run-p dev:*",
    "dev:css": "run-p \"generate:css:* --watch\"",
    "dev:node": "env-cmd -e dev ts-node src/server.ts",
    "dev:remix": "remix watch",
    "format": "prettier --write .",
    "graphql": "graphql-codegen",
    "generate:css:main": "tailwindcss -i ./src/styles/main.css -o ./src/styles/build/main.css",
    "generate:css:admin": "tailwindcss -i ./src/styles/admin.css -o ./src/styles/build/admin.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --report-unused-disable-directives .",
    "lint-fix": "yarn lint --fix",
    "prod": "run-s prod:*",
    "prod:install": "yarn",
    "prod:build": "yarn build",
    "prod:rsync": "gsutil -m rsync -a public-read -r public gs://wonderboymusic/highforthis",
    "prod:headers": "gsutil -m setmeta -r -h 'Cache-control:public, max-age=259200' gs://wonderboymusic/highforthis",
    "prod:stop": "forever stop highforthis",
    "prod:start": "env-cmd -e prod forever start -a forever.prod.json",
    "start": "env-cmd -e prod ts-node server.ts",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-update": "vitest run -u",
    "typecheck": "tsc -b",
    "validate": "run-p test lint typecheck"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/typescript": "2.8.6",
    "@remix-run/dev": "1.9.0",
    "@remix-run/eslint-config": "1.9.0",
    "@tailwindcss/forms": "0.5.3",
    "@testing-library/dom": "8.19.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/compression": "1.7.2",
    "@types/draft-js": "0.11.10",
    "@types/eslint": "8.4.10",
    "@types/express": "4.17.15",
    "@types/lodash.debounce": "4.0.7",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "3.0.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "8.1.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.4",
    "vite": "4.0.3",
    "vite-tsconfig-paths": "4.0.3",
    "vitest": "0.26.3"
  }
}
