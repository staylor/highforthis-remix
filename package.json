{
  "name": "highforthis",
  "version": "0.0.0",
  "description": "High for This",
  "repository": {
    "type": "git",
    "url": "https://github.com/staylor/highforthis"
  },
  "author": "Scott Taylor <scott.c.taylor@mac.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "dependencies": {
    "@apollo/client": "3.8.4",
    "@remix-run/express": "1.12.0",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "@remix-run/server-runtime": "1.12.0",
    "classnames": "2.3.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "draft-js": "0.11.7",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "filesize": "10.0.12",
    "forever": "4.0.3",
    "graphql": "16.8.1",
    "html-escape": "2.0.0",
    "http-proxy-middleware": "2.0.6",
    "lodash.debounce": "4.0.8",
    "morgan": "1.10.0",
    "qs": "6.11.2",
    "query-types": "0.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redraft": "0.10.2",
    "remix-auth": "3.5.1",
    "remix-auth-form": "1.3.0",
    "rimraf": "5.0.5",
    "tiny-invariant": "1.3.1"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "yarn clean",
    "build:css": "run-s \"generate:css:* --minify\"",
    "build:remix": "env-cmd -e prod remix build",
    "build:node": "env-cmd -e prod vite build --config vite.server.config.ts",
    "clean": "rimraf build public/build",
    "dev": "yarn clean && run-p dev:*",
    "dev:css": "run-p \"generate:css:* --watch\"",
    "dev:node": "env-cmd -e dev tsx server.ts",
    "dev:remix": "remix watch",
    "format": "prettier --write .",
    "graphql": "graphql-codegen",
    "generate:css:main": "tailwindcss -i ./src/styles/main.css -o ./src/styles/build/main.css",
    "generate:css:admin": "tailwindcss -i ./src/styles/admin.css -o ./src/styles/build/admin.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --report-unused-disable-directives .",
    "lint-fix": "yarn lint --fix",
    "prod": "run-s prod:*",
    "prod:install": "yarn",
    "prod:build": "yarn build",
    "prod:rsync": "gsutil -m rsync -a public-read -r public gs://wonderboymusic/highforthis",
    "prod:headers": "gsutil -m setmeta -r -h 'Cache-control:public, max-age=259200' gs://wonderboymusic/highforthis",
    "prod:stop": "forever stop highforthis",
    "prod:start": "env-cmd -e prod forever start -a forever.prod.json",
    "start": "env-cmd -e prod node server.js",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-update": "vitest run -u",
    "typecheck": "tsc -b",
    "validate": "run-p test lint typecheck"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@tailwindcss/forms": "0.5.6",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/compression": "1.7.3",
    "@types/cookie-parser": "1.4.4",
    "@types/draft-js": "0.11.13",
    "@types/eslint": "8.44.3",
    "@types/express": "4.17.18",
    "@types/html-escape": "2.0.0",
    "@types/lodash.debounce": "4.0.7",
    "@types/morgan": "1.9.6",
    "@types/node": "20.7.1",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "happy-dom": "12.2.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.30",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "tailwindcss": "3.3.3",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.13.0",
    "typescript": "4.9.5",
    "vite": "4.4.9",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.5"
  }
}
